<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    @Styles.Render("~/Content/feics/estilo.css")
    @Styles.Render("~/Content/bootstrap/css/bootstrap.min.css")
    @Scripts.Render("~/Content/bootstrap/js/bootstrap.min.js")
    

</head>
<body>
<div id="block-system-main" class="block block-system first last odd">
    <div class="content">
    <div id="titulo_pagina" class="clearfix">
    <div id="titulo_pagina_container">
        <h1>@ViewBag.Title</h1>
    </div>
</div>
  	    <div id="conteudo" class="clearfix">
            <div id="principal">
            @RenderBody()
            </div>
        </div>
    </div>
</div>


       
    
<!-- Footer -->


    @RenderSection("scripts", required: false)
    @Scripts.Render("http://code.jquery.com/jquery-1.10.2.min.js")

</body>
</html>
